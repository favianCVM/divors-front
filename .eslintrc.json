{
	"settings": {
		"import/resolver": {
			"alias": {
				"map": [
					["@", "./src"],
					["@pages/*", "./src/pages/*"],
					["@components/*", "./src/components/*"],
					["@models/*", "./src/models/*"],
					["@containers/*", "./src/containers/*"],
					["@utils/*", "./src/utils/*"],
					["@actions/*", "./src/state/actions/*"],
					["@reducers/*", "./src/state/reducers/*"],
					["@styles/*", "./src/styles/*"],
					["@assets/*", "./src/assets/*"]
				]
			}
		}
	}
}
